msgid "Mpesa WordPress Plugin"
msgstr "Mpesa WordPress Plugin"

#: mpesa-wp-plugin.php:96
msgid "Accept Mpesa payments for your store"
msgstr "Aceite pagamentos via Mpesa para a sua loja"

#: mpesa-wp-plugin.php:148
msgid "Enable/Disable"
msgstr "Habilitar/Desabilitar"

#: mpesa-wp-plugin.php:149
msgid "Enable Mpesa Wordpress plugin"
msgstr "Habilitar o Mpesa WordPress Plugin"

#: mpesa-wp-plugin.php:155
msgid "Title"
msgstr "Título"

#: mpesa-wp-plugin.php:157
msgid "This controls the title which the user sees during checkout"
msgstr "Isso controla o título que o usuário vê durante o pagamento"

#: mpesa-wp-plugin.php:158
msgid "Mpesa"
msgstr "Mpesa"

#: mpesa-wp-plugin.php:162
msgid "Customer Message"
msgstr "Mensagem para o cliente"

#: mpesa-wp-plugin.php:164
msgid "Insert your number below to proceed with checkout"
msgstr "Insira o seu número no campo abaixo para prosseguir com o pagamento"

#: mpesa-wp-plugin.php:167
msgid "API Key"
msgstr "Chave da API"

#: mpesa-wp-plugin.php:172
msgid "Public Key"
msgstr "Chave pública"

#: mpesa-wp-plugin.php:177
msgid "Service Provider Code"
msgstr "Código de provedor de serviços"

#: mpesa-wp-plugin.php:179
msgid "Use 171717 for testing"
msgstr "Use 171717 para testes"

#: mpesa-wp-plugin.php:183
msgid "Test Mode"
msgstr "Modo de testes"

#: mpesa-wp-plugin.php:185
msgid "Enable Test Environment"
msgstr "Habilitar ambiente de testes"

#: mpesa-wp-plugin.php:199
msgid "TEST MODE ENABLED"
msgstr "MODO DE TESTES HABILITADO"

#: mpesa-wp-plugin.php:205
msgid "Pay with Mpesa"
msgstr "Pague com Mpesa"

#: mpesa-wp-plugin.php:230
msgid "Mpesa Number"
msgstr "Número Mpesa"

#: mpesa-wp-plugin.php:253
msgid "Currency not supported!"
msgstr "Moeda não suportada!"

#: mpesa-wp-plugin.php:263
msgid "Phone number is required!"
msgstr "Número de telefone é obrigatório!"

#: mpesa-wp-plugin.php:284
msgid "Phone number is incorrect!"
msgstr "Número de telefone inválido!"

#: mpesa-wp-plugin.php:310
msgid "Payment Information"
msgstr "Informação de pagamento"

#: mpesa-wp-plugin.php:311
msgid "Thank you for your order, please click the button bellow to proceed."
msgstr ""
"Obrigado pela preferência, por favor clique no botão abaixo para prosseguir."

#: mpesa-wp-plugin.php:314
msgid "Payment request sent!"
msgstr "Pedido de pagamento enviado!"

#: mpesa-wp-plugin.php:317
msgid ""
"You will receive a pop-up on the phone requesting payment confirmation, "
"please enter your PIN code to confirm the payment."
msgstr ""
"Você irá receber uma janela na tela do seu celular para confirmar o "
"pagamento, por favor insira o seu PIN."

#: mpesa-wp-plugin.php:323
msgid "Payment received!"
msgstr "Pagamento recebido!"

#: mpesa-wp-plugin.php:324
msgid "Your payment has been received and your order will be processed soon."
msgstr ""
"O seu pagamento foi recebido e a sua compra será processada brevemente."

#: mpesa-wp-plugin.php:327
msgid "Payment timeout exceeded!"
msgstr "Tempo para pagamento esgotado!"

#: mpesa-wp-plugin.php:328
msgid "Use your browser's back button and try again."
msgstr "Use o botão de voltar do seu navegador e tente novamente."

#: mpesa-wp-plugin.php:331
msgid "Payment failed!"
msgstr "Pagamento falhou!"

#: mpesa-wp-plugin.php:332
msgid "Try again or use your browser's back button to change the number."
msgstr "Use o botão de voltar do seu navegador para alterar o número."

